<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="aga.integrador_juanvaldez.Cafe"
    android:background="#8c6239">

    <TextView
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:text="Latte"
        android:id="@+id/latte"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="false"
        android:layout_marginTop="48dp"
        android:textAlignment="center"
        android:background="@drawable/container"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_marginBottom="30dp" />

    <TextView
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:text="Capuccino"
        android:id="@+id/capuccino"
        android:textAlignment="center"
        android:background="@drawable/container"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_below="@+id/latte"
        android:layout_alignEnd="@+id/latte"
        android:layout_marginBottom="30dp" />

    <TextView
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:text="Expresso"
        android:id="@+id/expresso"
        android:textAlignment="center"
        android:background="@drawable/container"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_below="@+id/capuccino"
        android:layout_alignEnd="@+id/capuccino"
        android:layout_marginBottom="30dp" />

    <TextView
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:text="con Leche"
        android:id="@+id/leche"
        android:textAlignment="center"
        android:background="@drawable/container"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_below="@+id/expresso"
        android:layout_alignEnd="@+id/expresso"
        android:layout_marginBottom="30dp" />

    <TextView
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:id="@+id/mocca"
        android:textAlignment="center"
        android:background="@drawable/container"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_below="@+id/leche"
        android:layout_alignEnd="@+id/leche"
        android:layout_marginBottom="30dp"
        android:text="Mocca" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="0"
        android:id="@+id/contadorLatte"
        android:textAlignment="center"
        android:background="@drawable/contenedorSmall"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_marginLeft="30dp"
        android:layout_alignTop="@+id/latte"
        android:layout_toStartOf="@+id/RLatte"
        android:layout_marginRight="10dp" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/RLatte"
        android:background="@drawable/btnderecha"
        android:layout_alignBottom="@+id/contadorLatte"
        android:layout_alignParentEnd="true"
        android:onClick="latteDer" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/LLatte"
        android:background="@drawable/btnizquierda"
        android:layout_marginLeft="0dp"
        android:layout_alignBottom="@+id/contadorLatte"
        android:layout_alignStart="@+id/LCappuccino"
        android:onClick="latteIzq" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/LCappuccino"
        android:background="@drawable/btnizquierda"
        android:layout_marginLeft="20dp"
        android:layout_alignBottom="@+id/capuccino"
        android:layout_toEndOf="@+id/capuccino"
        android:onClick="capuccinoIzq" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/LExpresso"
        android:background="@drawable/btnizquierda"
        android:layout_marginLeft="20dp"
        android:layout_alignBottom="@+id/expresso"
        android:layout_toEndOf="@+id/expresso"
        android:onClick="expresoIzq" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/LLeche"
        android:background="@drawable/btnizquierda"
        android:layout_marginLeft="20dp"
        android:layout_alignBottom="@+id/leche"
        android:layout_toEndOf="@+id/leche"
        android:onClick="lecheIzq" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/LMocca"
        android:background="@drawable/btnizquierda"
        android:layout_marginLeft="20dp"
        android:layout_alignBottom="@+id/mocca"
        android:layout_toEndOf="@+id/mocca"
        android:onClick="moccaIzq" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="0"
        android:id="@+id/contadorCapuccino"
        android:textAlignment="center"
        android:background="@drawable/contenedorSmall"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_alignBottom="@+id/LCappuccino"
        android:layout_alignStart="@+id/contadorLatte" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="0"
        android:id="@+id/contadorExpresso"
        android:textAlignment="center"
        android:background="@drawable/contenedorSmall"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_alignBottom="@+id/LExpresso"
        android:layout_alignStart="@+id/contadorCapuccino" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="0"
        android:id="@+id/contadorLeche"
        android:textAlignment="center"
        android:background="@drawable/contenedorSmall"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_alignBottom="@+id/LLeche"
        android:layout_alignStart="@+id/contadorExpresso" />

    <TextView
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:text="0"
        android:id="@+id/contadorMocca"
        android:textAlignment="center"
        android:background="@drawable/contenedorSmall"
        android:textColor="#ffffff"
        android:textSize="25dp"
        android:padding="10dp"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="0dp"
        android:layout_alignTop="@+id/LMocca"
        android:layout_alignStart="@+id/contadorLeche" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/RCapuccino"
        android:background="@drawable/btnderecha"
        android:layout_alignBottom="@+id/contadorCapuccino"
        android:layout_toEndOf="@+id/contadorLatte"
        android:onClick="capuccinoDer" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/RExpresso"
        android:background="@drawable/btnderecha"
        android:layout_alignBottom="@+id/contadorExpresso"
        android:layout_toEndOf="@+id/contadorLatte"
        android:longClickable="false"
        android:onClick="expresoDer" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/RLeche"
        android:background="@drawable/btnderecha"
        android:layout_alignBottom="@+id/contadorLeche"
        android:layout_alignStart="@+id/RLatte"
        android:nestedScrollingEnabled="false"
        android:onClick="lecheDer" />

    <Button
        android:layout_width="35dp"
        android:layout_height="40dp"
        android:id="@+id/RMoccha"
        android:background="@drawable/btnderecha"
        android:layout_alignTop="@+id/contadorMocca"
        android:layout_toEndOf="@+id/contadorLatte"
        android:onClick="moccaDer" />

    <Button
        android:layout_width="200dp"
        android:layout_height="75dp"
        android:id="@+id/button11"
        android:background="@drawable/btncomprar"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:onClick="comprarCafe" />

</RelativeLayout>
